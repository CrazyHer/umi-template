    "rap": "^1.3.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "umi": "^3.5.23"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "^1.0.0",
    "@umijs/test": "^3.5.23",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
